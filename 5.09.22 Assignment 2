#include <stdio.h>  
  
void  main()  
{  
    int arr[10],n; 
    int i;  
    printf("The size of the array");
    scanf("%d",&n);
    for(i=0; i<n; i++)  
    {  
	    printf("element %d : ",i);
        scanf("%d", &arr[i]);  
    }  
  
    printf("\nElements in array are: ");  
    for(i=0; i<n; i++)  
    {  
        printf("%d  ", arr[i]);  
    } 
    printf("\n");	
}
